{
  "name": "airfoil-typescript-sdk",
  "private": true,
  "version": "0.0.0",
  "description": "Airfoil Typescript monorepo",
  "scripts": {
    "build": "turbo run build",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test",
    "test:ci": "turbo run test:ci",
    "format": "bun --filter '*' format",
    "lint": "bun --filter '*' lint",
    "lint:fix": "bun --filter '*' lint:fix"
  },
  "workspaces": [
    "packages/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/usearifoil/typescript-sdk.git"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.5",
    "@types/node": "^24.7.0",
    "turbo": "^2.5.8",
    "typescript": "^5.9.2"
  },
  "packageManager": "bun@1.3.5"
}