{
  "name": "airfoil",
  "version": "0.1.1",
  "module": "index.ts",
  "type": "module",
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "bin": {
    "airfoil": "./dist/index.js"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "pnpm lint --write",
    "format": "biome format --write .",
    "airfoil:dev": "bun run src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target node",
    "compile": "bun build src/index.ts --compile --outfile airfoil",
    "compile:all": "bun run src/scripts/compile-all.ts"
  },
  "devDependencies": {
    "@types/bun": "^1.3.5"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@useairfoil/flight": "workspace:*",
    "@useairfoil/wings": "workspace:*",
    "@clack/prompts": "^0.11.0",
    "@effect/cli": "^0.73.1",
    "@effect/platform-node": "^0.104.1",
    "console-table-printer": "^2.15.0",
    "effect": "^3.19.15",
    "nice-grpc": "^2.1.13",
    "nice-grpc-common": "^2.0.2"
  }
}
