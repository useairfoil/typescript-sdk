{
  "name": "@airfoil/cli",
  "version": "0.1.0",
  "module": "index.ts",
  "type": "module",
  "bin": {
    "airfoil": "./dist/index.js"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "pnpm lint --write",
    "format": "biome format --write .",
    "airfoil:dev": "bun run src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target node",
    "compile": "bun build src/index.ts --compile --outfile airfoil",
    "compile:all": "bun run scripts/compile-all.ts"
  },
  "devDependencies": {
    "@types/bun": "^1.3.5"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "commander": "^14.0.2"
  }
}